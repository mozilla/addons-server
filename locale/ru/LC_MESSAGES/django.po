msgid_plural "{0} errors"
msgstr[0] "{0} ошибка"
msgstr[1] "{0} ошибки"
msgstr[2] "{0} ошибок"

#. L10n: first parameter is the number of warnings
# {0} is a number
msgid "{0} warning"
msgid_plural "{0} warnings"
msgstr[0] "{0} предупреждение"
