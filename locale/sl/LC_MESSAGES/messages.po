msgstr "Ocenjeno na %d od 5 zvezdic"

#: apps/reviews/forms.py:120
msgid "Keep review; remove flags"
msgstr "Zadrži komentar; odstrani zastavice"

#: apps/reviews/forms.py:121
msgid "Skip for now"
msgstr "Preskoči za zdaj"

#: apps/reviews/forms.py:123 apps/reviews/templates/reviews/review.html:94
msgid "Delete review"
msgstr "Izbriši pregled"

#: apps/reviews/models.py:134
msgid "Other (please specify)"
