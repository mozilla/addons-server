msgstr[2] "Ta uporabnik ima <a href=\"%(user_review_url)s\">%(cnt)s prejšnje ocene</a> za ta dodatek."
msgstr[3] "Ta uporabnik ima <a href=\"%(user_review_url)s\">%(cnt)s prejšnjih ocen</a> za ta dodatek."

#, python-format
msgid "This user has <a href=\"%(user_review_url)s\">other reviews</a> of this add-on."
msgstr "Ta uporabnik ima <a href=\"%(user_review_url)s\">druge ocene</a> za ta dodatek."

