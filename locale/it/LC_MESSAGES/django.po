#: src/olympia/devhub/templates/devhub/includes/addons_create_profile.html
msgid "Create a Developer Profile"
msgstr "Crea un profilo sviluppatore"

#: src/olympia/devhub/templates/devhub/includes/addons_create_profile.html
msgid ""
