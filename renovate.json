{
  "extends": [
    "config:base",
    ":maintainLockFilesWeekly",
    ":prNotPending",
    "docker:disable"
  ],
  "packageRules": [
    {
      "packagePatterns": [
        "*"
      ],
      "rangeStrategy": "pin"
    },
    {
      "depTypeList": [
        "engines",
        "devDependencies"
      ],
      "rangeStrategy": "replace"
    }
  ],
  "pip_requirements": {
    "fileMatch": [
      "^requirements/.*\\.txt$"
    ]
  }
}
